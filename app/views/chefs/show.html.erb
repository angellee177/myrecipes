<%= render 'shared/page_title', title: "Chef #{@chef.chef_name}'s Profile"%>
<div class="row">
    <div class="col-md-4 col-md-offset-4 center">
    <% if @chef.picture.present? %>
        <%= image_tag(@chef.picture_url, size:150, class:'img')%>
    <% else %>
        <%= gravatar_for @chef, size: 150 %>
    <% end %>
    </div>
</div>

<h4 style="text-align:center"><%= @chef.chef_name %>'s Recipes</h4>


<%= render 'recipes/recipe', obj:@chef_recipes %>